    "@nestjs/core": "^10.3.3",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^2.4.3",
