    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sonyflake": "^1.1.2",
    "zod": "^3.22.4"
